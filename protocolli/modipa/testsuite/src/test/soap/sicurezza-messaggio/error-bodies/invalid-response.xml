<env:Envelope xmlns:env="http://www.w3.org/2003/05/soap-envelope">
    <env:Header/>
    <env:Body>
        <env:Fault>
            <env:Code>
                <env:Value>env:Receiver</env:Value>
                <env:Subcode>
                    <env:Value xmlns:integration="http://govway.org/integration/fault">integration:InvalidResponse</env:Value>
                </env:Subcode>
            </env:Code>
            <env:Reason>
                <env:Text xml:lang="en-US">Invalid response received from the API Implementation</env:Text>
            </env:Reason>
            <env:Role>http://govway.org/integration</env:Role>
            <env:Detail>
                <problem xmlns="urn:ietf:rfc:7807">
                    <type>https://govway.org/handling-errors/502/InvalidResponse.html</type>
                    <title>InvalidResponse</title>
                    <status>502</status>
                    <detail>Invalid response received from the API Implementation</detail>
                    <govway_id>#string</govway_id>
                </problem>
            </env:Detail>
        </env:Fault>
    </env:Body>
</env:Envelope>
