{
	"connettore": {
		"endpoint": "https://ginovadifretta.it/petstore"
	},
	"autenticazione": {
		"tipo": "https",
		"opzionale": false
	},
	"api_nome": "PetStore",
	"api_versione": 2,
	"modi": {
		"protocollo": "rest",
		"richiesta": {
			"sicurezza_messaggio": {
				"riferimento_x509": [
					"x5c"
				],
				"informazioni_utente_userid": {
					"modalita": "ridefinito",
					"valore": "userid"
				}
			}
		},
		"risposta": {
			"sicurezza_messaggio": {
				"truststore": {
					"modalita": "default"
				}
			}
		}
	}
}
